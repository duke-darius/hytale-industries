$C = "../Common.ui";

Group {
  Anchor: (Width: 320, Height: 160);

  $C.@Container {
    Anchor: (Full: 0);

    #Title {
      Group {
        $C.@Title {
          @Text = "Small Battery";
        }
      }
    }

    #Content {
      LayoutMode: Top;
      Padding: (Left: 12, Right: 12, Top: 12, Bottom: 12);

      $C.@Title #HeText {
        @Alignment = Start;
        @Text = "HE: 0 / 1000000";
      }
    }
  }
}
